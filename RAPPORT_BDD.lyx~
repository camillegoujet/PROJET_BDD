\end_inset

SELECT p.Nom AS Nom_Pays, IDH
\begin_inset Newline newline
\end_inset

FROM Pays AS p
\begin_inset Newline newline
\end_inset

INNER JOIN former AS f ON f.IDPays = p.IDPays
\begin_inset Newline newline
\end_inset

INNER JOIN Alliance AS a ON a.IDAlliance = f.IDAlliance
\begin_inset Newline newline
\end_inset

WHERE a.Nom='BRICS'
\begin_inset Newline newline
\end_inset

ORDER BY IDH;
