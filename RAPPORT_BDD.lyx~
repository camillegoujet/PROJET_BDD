\begin_inset Newline newline
\end_inset

NOT IN (SELECT p.Nom AS Nom_Pays FROM Pays
\begin_inset Newline newline
\end_inset

INNER JOIN former AS f ON f.IDPays = p.IDPays
\begin_inset Newline newline
\end_inset

INNER JOIN Alliance AS a ON a.IDAlliance = f.IDAlliance
\begin_inset Newline newline
\end_inset

WHERE a.Nom = 'OTAN');
